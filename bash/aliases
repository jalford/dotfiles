alias ct='cd /Volumes/Code/TSD'
alias ..='cd ..'
alias c='clear'
alias cdd='cd -'  # goto last dir cd'ed from
alias cll='clear; lh'
alias cl='clear; ls'
alias h='history'
alias m="mate"
alias m.="mate ."
alias l.='ls -d .[^.]*'
alias lah='ls -lah'
alias lh="ls -lh"
alias ll='ls -lhG'
alias lt='ls -lt'
alias md='mkdir -p'
alias o='open . &'
alias ea='mate -w ~/bin/dotfiles/bash/aliases && reload'
alias ra='source ~/bin/dotfiles/bash/aliases'
alias e='exit'
alias gs='git status'
alias gb='git branch'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gco='git checkout'
alias gb='git branch'
alias ungit="find . -name '.git' -exec rm -rf {} \;"
alias gd='git diff'
alias gdm='git diff master'
function mdcd() { 
    mkdir -p "$1"
    cd "$1" 
}





